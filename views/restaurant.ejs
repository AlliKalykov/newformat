<!DOCTYPE html>
<html>
<head>
    <%- include head.ejs%>
</head>
<body>
<%- include header.ejs%>
    <div class="blog-w3ls py-5 bg-light" id="news">
        <div class="container py-xl-1 py-lg-3 text-center">
            <!-- heading title -->
            <div class="main-title text-center mb-5">
                <h3 class="tittle text-uppercase font-weight-bold"><%= title%></h3>
                <div class="title-icon">
                    <i class="fas fa-camera"></i>
                </div>
                <p class="sub-tittle"><%- subText %></p>
            </div>
            <!-- //heading title -->
            <div class="row package-grids">
            <% restObj.forEach(element => {%>
                <%- include("restaurSingle",{restHref: element.restHref, restTitle: element.restTitle,
                    image: element.image, facebook: element.facebook, instagram: element.instagram}) %>
            <%});%>
            </div>
        </div>
    </div>
<%- include footer.ejs%>
</body>
</html>
